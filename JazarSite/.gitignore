# Ignore node modules
node_modules/

# Ignore build and dist files
dist/
build/

# Ignore environment files
.env
.env.local
.env.development
.env.production

# Ignore npm and yarn logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore lock files (optional, but recommended for consistency)
package-lock.json
yarn.lock
